def List-L0-Int [ List(Int) ] { List.L0 }
def List-L0-Str [ List(Str) ] { List.L0 }
def None-Int [ Maybe(Int) ] { None }

def Even/Odd [ Int -- Either(Int,Int) ] { 2 /% 0= if(Left, Right) }
