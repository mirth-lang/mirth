module test.table-syntax

import std.prelude
import std.world

table Foo1
table Foo2 { num: Int }
table Foo3 { num: Int { index >Int } }
table Foo4 {
    num: Int
    num2: Int
    ~raw: Int
    --
    def num^2 { num dup * }
    def raw { ~raw @ }
    def Create! [ Foo4 ] { 10 >num 20 >num2 Foo4.Make! }
}

def main {
    Foo4.Create! num2
    print("Foo4.Create! num2 = "; int;)
}
