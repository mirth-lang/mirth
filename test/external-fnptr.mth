module(mirth-tests.external-fnptr)

import(std.prelude)
import(std.posix)

external(
    atexit [ +World [ +World -- +World ] -- +World ]
)

def(main, +World -- +World,
    [ "goodbye world" print ] atexit
    "hello world" print)

