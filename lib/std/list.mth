struct L2(t) {
    t t
    --
    def Cons [ t L1(t) -- L2(t) ] { L1 -> L2 }
    def Snoc [ L1(t) t -- L2(t) ] { dip:/L1 L2 }
    def cons [ t L2(t) -- L3(t) ] { L2 -> L3 }
    def xsnoc [ L2(t) x:t -- L3(t) ] { L2 -> x> L3 }

    def >List [ L2(t) -- List(t) ] { L2 -> List.L2  }
