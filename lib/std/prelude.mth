    def(F32.>=, F32 F32 -- Bool, < not)
    def(F32.<>, F32 F32 -- Bool, = not)

    def(Int.max, Int Int -- Int, dup2 < if(nip, drop))
    def(Int.min, Int Int -- Int, dup2 < if(drop, nip))
    def(Int.in-range, Int Int Int -- Bool, dip(over dip(>=)) <= &&)
)

