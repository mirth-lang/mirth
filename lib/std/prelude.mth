    def Int.>Nat (f,g) [ ( *a Nat -- *b, *a Int -- *b ) *a Int -- *b ] { dup 0< if(g, NatUnsafe f) }

    def Nat.>Int [ Nat -- Int ] { /NatUnsafe }

    def Nat.>U8  [ Nat -- U8  ] { >Int >U8  }
    def Nat.>U16 [ Nat -- U16 ] { >Int >U16 }
