    def(Int.compare, Int Int -- Comparison, dup2 == if(drop2 EQ, < if(LT, GT)))

    alias(F64.==, prim-f64-eq)
    alias(F64.=, prim-f64-eq)
    alias(F64.<, prim-f64-lt)
    def(F64.>, F64 F64 -- Bool, swap <)
    def(F64.<=, F64 F64 -- Bool, swap < not)
