    def U32.>Nat [ U32 -- Nat ] { >Int NatUnsafe }
    def U64.>Nat [ U64 -- Nat ] { >Int-unsafe NatUnsafe }

    def Nat.+ [ Nat Nat -- Nat ] { both(>Int) + NatUnsafe }
    def Nat.- [ Nat Nat -- Int ] { both(>Int) - }
    def Nat.* [ Nat Nat -- Nat ] { both(>Int) * NatUnsafe }
