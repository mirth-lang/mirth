    def * (f1{*},f2{*},f3{.i},f4{+}) [
        (*z x a -- *z xa,
         *z x b -- *z xb,
         *z xb -- *z xbi,
         *z xa xbi -- *z y)
        *z x Complex(a,b) -- *z y
    ] {
        /Complex real> over imag>
        dip2(f1) rotl dip(f2 f3) swap f4
    }

    def / (f{invert}, g{*}) [
        ( *z Complex(a,b) -- *z Complex(a',b')
        , *z x Complex(a', b') -- *z y
