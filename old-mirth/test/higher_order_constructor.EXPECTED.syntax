data T
  A(f: Bool -- Bool) : T
end
constT : Bool -- T
constT = if(A(drop true), A(drop false))
isoT : Bool -- T
isoT = if(A(id), A(not))
runT : Bool T -- Bool
runT = match(A(f) -> f)
constT runT == nip
isoT dup dip(runT) runT == drop
runTthen(g: Bool -- Bool) : Bool T -- Bool
runTthen(g) = match(A(f) -> f g)
constT runTthen(id) == nip
constT runTthen(not) == nip not
